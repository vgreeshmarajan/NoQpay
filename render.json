{
  "name": "noqapp",
  "type": "web",
  "buildCommand": "npm install",
  "startCommand": "expo start --no-dev --minify",
  "env": {
    "NODE_ENV": "production"
  },
  "web": {
    "port": 80,
    "buildCommand": "npm run build",
    "startCommand": "expo start --no-dev --minify",
    "env": {
      "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}",
      "EXPO_PUBLIC_API_KEY": "${EXPO_PUBLIC_API_KEY}"
    }
  }
}
