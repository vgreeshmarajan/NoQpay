{
  "name": "noqapp",
  "type": "web",
  "buildCommand": "npm install && npm run build",
  "startCommand": "expo start --no-dev --minify --offline",
  "env": {
    "NODE_ENV": "production",
    "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}",
    "EXPO_PUBLIC_API_KEY": "${EXPO_PUBLIC_API_KEY}"
  },
  "web": {
    "port": 80,
    "buildCommand": "npm run build",
    "startCommand": "npx expo start --no-dev --minify",
    "env": {
      "NODE_ENV": "production"
    }
  }
}
